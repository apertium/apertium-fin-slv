   <section id="main" type="standard">

    <!-- problems: mismatches -->

     <e><p><l>kyllä<s n="adv"/></l><r>da<s n="part"/></r></p></e>
     <e><p><l>ei<s n="adv"/></l><r>ne<s n="part"/></r></p></e>
